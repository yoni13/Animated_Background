{
    "name": "Nice background",
    "description": "Let every website has a beautiful animated background.",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
          "js": ["inject.js"]
      }
    ],
    "host_permissions": ["<all_urls>"]
  }